<p id="notice"><%= notice %></p>

<h1>Items</h1>

<table>
  <thead>
    <tr>
      <th>Web ID</th>
      <th>Title</th>
      <th>Description</th>
      <th>Duration</th>
      <th>Web added</th>
      <th>Priority</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.web_id %></td>
        <td><%= item.title %></td>
        <td><%= item.description %></td>
        <td><%= item.duration %></td>
        <td><%= item.web_added %></td>
        <td><%= item.priority %></td>
        <td><%= link_to 'Show', item %></td>
        <td><%= link_to 'Edit', edit_item_path(item) %></td>
        <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_item_path %>
